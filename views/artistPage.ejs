<html>
  <head>
    <title><%= artist.name %></title>
    <link rel="stylesheet" href="/styles.css" />
  </head>
  <body>
    <h1><%= artist.name %></h1>
    <div>
        <h1>Music</h1>
        
    <ul>
        <% music.forEach((album) => { %>
            <li>
                <p><%= album.albumname %></p>
                <p><%= album.type %></p>
                <p><%= album.name%></p>
                <p><%= album.releasedate%></p>
                <a href="/albums/<%= album.musicid %>/update">Update</a>
                <a href="/albums/<%= album.musicid %>/delete">Delete</a>
            </li>
        <% }); %>
    </ul>
    </div>
  </body>
</html>
